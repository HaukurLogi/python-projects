nafn = input(" Nafn: ") 
import random
rng = random.randint(1,3)
x = 8
if x == 8:
    print ("Allt í lagi,", nafn, "Spurningin er hver er sætari. Ef þú ert tilbúin skrifaðu 1.")

tilbúin = input(" if you dare: ")


if rng == 1 and tilbúin == ("1"):
    n = "BibbiSZ08"
if rng == 2 and tilbúin == ("1"):
    n = "Team M"
if rng == 3 and tilbúin == ("1"):
    n = "CrestedPants_79"
print("Svarið er", n)
